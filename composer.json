{
    "name": "stacktrace/laravel-navigation",
    "description": "Opinionated navigation library for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Stovka",
            "email": "ps@stacktrace.sk"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "StackTrace\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StackTrace\\Navigation\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StackTrace\\Navigation\\NavigationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
